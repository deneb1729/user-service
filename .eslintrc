{
    "env": {
        "node": true,
        "es6": true,
        "browser": true,
        "jest": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "no-console": 0,
        "prettier/prettier": "error",
        "import/prefer-default-export": 0
    },
    "ignorePatterns": "dist/*",
    "parserOptions": {
        "ecmaVersion": 2019
    }
}